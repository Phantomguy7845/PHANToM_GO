<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/aurora_purple">
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:angle="135"
                android:startColor="@color/aurora_blue"
                android:centerColor="@color/aurora_cyan"
                android:endColor="@color/aurora_green"
                android:type="linear" />
            <corners android:radius="16dp" />
        </shape>
    </item>
</ripple>
