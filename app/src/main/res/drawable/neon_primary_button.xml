<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/aurora_cyan">
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:startColor="@color/aurora_mint"
                android:endColor="@color/aurora_cyan"
                android:angle="0" />
            <stroke
                android:width="1dp"
                android:color="@color/aurora_cyan" />
            <corners android:radius="18dp" />
        </shape>
    </item>
</ripple>
